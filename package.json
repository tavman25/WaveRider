{
  "name": "waverider",
  "version": "0.1.0",
  "description": "WaveRider - An Agentic AI-Native IDE for Autonomous Development",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "cd backend && python -m uvicorn server:app --reload --port 8002",
    "agents": "cd agents && python -m uvicorn agent_server:app --reload --port 8001",
    "dev:full": "concurrently \"npm run backend\" \"npm run agents\" \"npm run dev\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:backend": "cd backend && python -m pytest",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "deploy": "npm run build && npm run test && echo 'Ready for deployment'",
    "stop": "taskkill /f /im node.exe /im python.exe 2>nul || echo 'Processes stopped'",
    "deploy:local": "scripts/deploy.bat",
    "deploy:prod": "echo 'Deploy to production with GitHub Actions'"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "framer-motion": "^11.3.2",
    "lucide-react": "^0.408.0",
    "monaco-editor": "^0.50.0",
    "next": "^14.2.5",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-query": "^3.39.3",
    "react-syntax-highlighter": "^15.5.0",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3"
  },
  "keywords": [
    "ai",
    "ide",
    "code-editor",
    "autonomous-agents",
    "langchain",
    "typescript",
    "next.js",
    "fastapi",
    "agentic-ai"
  ],
  "author": "WaveRider Team",
  "license": "MIT"
}
